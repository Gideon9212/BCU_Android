<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/stgenlistexp"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/cell_shape"
            android:contentDescription="@string/def_description"
            app:srcCompat="@drawable/ic_expand_less_black_24dp"
            app:tint="?attr/iconPrimary" />

        <ImageView
            android:id="@+id/stgenlisticon"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.875"
            android:background="@drawable/cell_shape"
            android:contentDescription="@string/def_description"
            android:paddingStart="12dp"
            android:paddingTop="8dp"
            android:paddingEnd="12dp"
            android:paddingBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_castle" />

        <TableRow
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2.5">

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/stgenlistmultir"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:inputType="number"
                android:minHeight="48dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/stgenlistnumr"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:inputType="text"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />
        </TableRow>

        <ImageButton
            android:id="@+id/stgenlistinfo"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.625"
            android:background="@drawable/cell_shape"
            android:contentDescription="@string/def_description"
            android:paddingStart="8dp"
            android:paddingTop="8dp"
            android:paddingEnd="8dp"
            android:paddingBottom="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_info_outline_black_24dp"
            app:tint="?attr/iconPrimary" />

    </TableRow>

    <TableLayout
        android:id="@+id/stgenlistmi"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:stretchColumns="*"
        app:layout_constraintBottom_toTopOf="@id/enemlistrevcont"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow5">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/enemlistbh"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/stg_info_bh"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/enemlistib"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/stg_info_isboss"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/enemlistlay"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/stg_info_layer"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemlistbhr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <Spinner
                android:id="@+id/enemlistibr"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@drawable/cell_spinner"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemlistlayr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/enemlistst"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.375"
                android:background="@drawable/button_shape"
                android:gravity="center"
                android:text="@string/stg_info_start"
                android:textAllCaps="false"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/enemlistkilc"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.25"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/stg_info_killcount"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/enemlistres"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.375"
                android:background="@drawable/button_shape"
                android:gravity="center"
                android:text="@string/stg_info_respawn"
                android:textAllCaps="false"
                android:textColor="?attr/TextPrimary"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemliststr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemlistkilcr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:inputType="number"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemlistresr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.375"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/enemlistedoor"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.4"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/spawn_door"
                android:textColor="?attr/TextPrimary" />

            <TextView
                android:id="@+id/enemlistrv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:text="@string/eff_revive"
                android:textColor="?attr/TextPrimary" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.g2.bcu.androidutil.supports.WatcherEditText
                android:id="@+id/enemlistevrdr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.4"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:textColor="?attr/TextPrimary"
                android:textColorHint="?attr/HintPrimary" />

            <ImageButton
                android:id="@+id/enemlistrev"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.1"
                android:background="@drawable/cell_shape"
                android:contentDescription="@string/def_description"
                app:srcCompat="@drawable/ic_expand_less_black_24dp"
                app:tint="?attr/iconPrimary" />
        </TableRow>
    </TableLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/enemlistrevcont"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
